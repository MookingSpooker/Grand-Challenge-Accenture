    "dfs = {}\n",
    "for path in csv_paths:\n",
    "    name = path.stem\n",
    "    df = pd.read_csv(path)\n",
    "    dfs[name] = df\n",
    "    print(f\"Loaded '{name}' with shape {df.shape}\")"
